#! /bin/zsh

export LANG=en_US.UTF-8

[ -r ~/.functions.sh ] && source ~/.functions.sh
[ -r ~/.aliases.sh ] && source ~/.aliases.sh
[ -r ~/.environment.sh ] && source ~/.environment.sh

export ZSH=~/.oh-my-zsh

ZSH_THEME="matts"

HYPHEN_INSENSITIVE="true"

DISABLE_AUTO_UPDATE="true"

ENABLE_CORRECTION="true"

COMPLETION_WAITING_DOTS="true"

HIST_STAMPS="mm/dd/yyyy"

plugins=(git colored-man-pages colorize command-not-found cp extract history history-substring-search tmux redis-cli fasd zsh-syntax-highlighting zsh-autosuggestions zsh-completions my-completions)
