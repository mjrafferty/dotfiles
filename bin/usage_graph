#! /bin/awk -f

BEGIN {

  inc=0;
  max=0;
}

{
  time[inc]=$1;
  hits[inc]=$2;

  if ($2>max)
  {
    max=$2
  }
  inc++
}

END {

  GRAY="\033[1;30m"
  RED="\033[1;31m"
  GREEN="\033[1;32m"
  YELLOW="\033[1;33m"
  BLUE="\033[1;34m"
  PURPLE="\033[1;35m"
  CYAN="\033[1;36m"
  WHITE="\033[1;37m"
  RESET="\033[0m "

  printf "\n";
  for (x=0;x<=inc;x++)
  {
    printf "%s\t%s\t",WHITE time[x],BLUE hits[x];
    for (i=0; i<(hits[x] * 100 / max);i++)
    {
      printf "%s",GREEN"*";
    }
    printf "\n"
  }
}
