#! /bin/bash

readonly ARGS="$*"
readonly ARGA=("$@")

main () {

  if [[ -z "$ARGS" || "$ARGS" =~ --help$|-h$ ]]; then
    _usage;
    return 0;
  fi

  local file;

  file="${ARGA[0]}"

  awk '{ gsub(/<|>/,"",$(NF)); printf "%10.6f %10.6f\n",(total_sum += $2),(user_sum += ($2 - $(NF))) }' "$file" \
    | paste - "$file" \
    | less -S;

}

main;
