#! /bin/bash

readonly ARGS="$*"
readonly ARGA=("$@")

main () {

  if [[ -z "$ARGS" || "$ARGS" =~ --help$|-h$ ]]; then
    _usage;
    return 0;
  fi

  echo;

  zgrep -h " = -1 E" ${ARGA[*]}\
    | grep -Ev "( (access|lstat|stat)\(|(EAGAIN|EOPNOTSUPP|EINPROGRESS))" \
    | less -SF

  echo;

}

main;
