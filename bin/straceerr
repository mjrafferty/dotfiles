_usage() {
  cat <<- EOF
  This is the usage
EOF
}

main () {

  if [[ -z "$ARGS" || "$ARGS" =~ --help$|-h$ ]]; then
    _usage;
    return 0;
  fi

  echo;
  zless ${ARGA[*]} \
    | grep " = -1 E" \
    | grep -Ev "(access)\(" \
    | less -SF

  echo;

}

main;
