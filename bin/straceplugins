#! /bin/bash

readonly ARGS="$*"
readonly ARGA=("$@")

main () {

  straceopened ${ARGA[*]} \
    | sed -n \
    -e 's_app/code/local/\([^/]*/[^/]*\)/.*_\1_p' \
    -e 's_app/code/community/\([^/]*/[^/]*\)/.*_\1_p' \
    -e 's_wp-content/plugins/\([^/]*\)/.*_\1_p' \
    | sort \
    | uniq -c \
    | sort -hr


}

main;
