#! /bin/bash

readonly ARGS="$*"
readonly ARGA=("$@")

readonly SED='/bin/sed'
readonly SORT='/bin/sort'

main () {

  straceopened "${ARGA[@]}" \
    | "$SED" -n \
    -e 's_app/code/local/\([^/]*/[^/]*\)/.*_\1_p' \
    -e 's_app/code/community/\([^/]*/[^/]*\)/.*_\1_p' \
    -e 's_wp-content/plugins/\([^/]*\)/.*_\1_p' \
    | "$SORT" \
    | uniq -c \
    | "$SORT" -hr

}

main;
