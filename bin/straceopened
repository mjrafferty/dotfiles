#! /bin/bash

readonly ARGS="$*"
readonly ARGA=("$@")

_usage() {
	cat <<- EOF
    This is the usage
	EOF
}

main () {

  if [[ -z "$ARGS" || "$ARGS" =~ --help$|-h$ ]]; then
    _usage;
    return 0;
  fi


  grep -Pio 'open\(\"/chroot/home/[^/]*/[^/]*/[^/]*/\K[^"]*' "${ARGA[0]}" \
    | sort -u

}

main;
